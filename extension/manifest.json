{
  "name": "ScrollPoint",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Reenables scrolling on most SharePoint sites. Icon by IconShock.com",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
  	"scripts": ["spoint_bg.js"],
  	"persistent": false
  },
  "permissions": [
  	"tabs",
	"webNavigation",
	"http://*/",
	"https://*/"
  ]
}